@model GameInfoViewModel

@{
	ViewData["Title"] = $"{Model!.Info.Title}";
}

@section Head
{
	<script src="~/js/signalr/dist/browser/signalr.js"></script>
	<partial name="_MultiplayerGame" />
	@{
		string style = $"{Model.Info.RouteId}.css";
		string head = $"_{Model.Info.RouteId}-Head";
	}
	<link rel="stylesheet" href="~/css/@style">
	<partial name="@head"></partial>
	<partial name="_Joke-API-Head" />
}

@{ string gameplay = $"_{Model.Info.RouteId}.cshtml"; }
<partial name="@gameplay" />
<br>
<partial name="_Joke-API" />

@section Scripts
{
	@{ string src = $"{Model.Info.RouteId}.js"; }
	<script src="~/js/hubs/@src"></script>
}
